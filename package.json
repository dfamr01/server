{
  "name": "steamme-server",
  "version": "0.0.1",
  "private": true,
  "description": "Stream me server",
  "nodemonConfig": {
    "ignore": [
      "/primus/primus.js",
      "currencies",
      "temp"
    ]
  },
  "scripts": {
    "start": "npm run start:run",
    "start:run": "node server/server.js",
    "start:test": "cross-env NODE_ENV=test npm run debug:server",
    "start:all-tests": "cross-env NODE_ENV=development nodemon --inspect=3000 server/server.js --exec 'npm run lint && node'",
    "debug:server": "cross-env NODE_ENV=development nodemon --inspect=3000 server/server.js",
    "debug:server:local": "cross-env NODE_ENV=local nodemon --inspect=3000 server/server.js",
    "lint": "cross-env NODE_ENV=development eslint --ext .js --color --format=node_modules/eslint-formatter-pretty server",
    "lint:watch": "npm run lint --watch",
    "lint:test": "npm-run-all --parallel lint:watch",
    "test:unit": "cross-env NODE_ENV=test mocha --time --delay --exit \"server/**/*.spec*.js\" --timeout 0",
    "test": "npm-run-all --parallel lint test:unit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.279.0",
    "@aws-sdk/lib-storage": "^3.279.0",
    "@aws-sdk/node-http-handler": "^3.272.0",
    "@aws-sdk/s3-request-presigner": "^3.279.0",
    "@aws-sdk/xhr-http-handler": "^3.274.0",
    "@sendgrid/mail": "^7.4.5",
    "@socket.io/redis-adapter": "^7.2.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "busboy": "^1.6.0",
    "cloudinary": "^1.22.0",
    "compression": "^1.7.4",
    "connect-flash": "^0.1.1",
    "connect-redis": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "elasticsearch": "^16.7.1",
    "engine.io": "^6.1.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^3.23.3",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^8.5.1",
    "jsrsasign": "^10.5.17",
    "libphonenumber-js": "^1.7.55",
    "lodash": "^4.17.19",
    "log4js": "^6.3.0",
    "method-override": "^3.0.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "money": "^0.2.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.0",
    "primus": "^8.0.5",
    "qs": "^6.9.4",
    "sequelize": "^6.3.3",
    "sm-validators": "0.0.3",
    "socket.io": "^4.5.3",
    "stripe": "^8.222.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "colors": "^1.4.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-formatter-pretty": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "faker": "^4.1.0",
    "mocha": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "should": "^13.2.3",
    "supertest": "^4.0.2",
    "uglify-js": "^3.14.5"
  },
  "repository": {
    "type": "git",
    "url": ""
  }
}
