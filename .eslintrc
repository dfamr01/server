{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": false,
        "node": true,
        "jquery": true,
        "mocha": true,
        "worker": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "plugins": [
        "flowtype",
        "promise",
        "compat"
    ],
    "rules": {}
}